{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-ide-helper": "^2.4",
        "box/spout": "^3.0",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "~4.0",
        "furqansiddiqui/erc20-php": "^0.1.2",
        "furqansiddiqui/ethereum-rpc": "dev-master",
        "jacobcyl/ali-oss-storage": "^2.1",
        "kylekatarnls/laravel-carbon-2": "^1.0.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "^3.1",
        "naux/sendcloud": "^1.1",
        "nesbot/carbon": "2.16.3 as 1.39.1",
        "overtrue/laravel-pinyin": "~3.0",
        "overtrue/laravel-wechat": "^4.0",
        "pragmarx/google2fa": "^5.0",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^0.22.1",
        "rookiejin/mipush": "^1.0",
        "sentry/sentry-laravel": "^0.11.0",
        "shaozeming/laravel-getui": "^1.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "stevenyangecho/laravel-u-editor": "^1.4",
        "yansongda/pay": "^2.6",
        "zhuzhichao/ip-location-zh": "^2.4"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "nunomaduro/collision": "~1.1",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "files":[
            "app/Libs/functions.php",
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
